# KaTrain Docker Deployment Environment Variables
# Copy this file to .env and fill in your values

# PostgreSQL password (change in production!)
POSTGRES_PASSWORD=katrain_secure_password_CHANGE_ME

# DashScope API key for LLM translation (optional, translation will skip if not set)
DASHSCOPE_API_KEY=sk-your-dashscope-api-key

# Path to KataGo models directory (contains .bin.gz files)
KATAGO_MODEL_PATH=./katago-models
